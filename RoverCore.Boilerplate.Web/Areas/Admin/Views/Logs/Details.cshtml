@model RoverCore.Boilerplate.Domain.Entities.Serilog.Log

@{
    ViewData["Title"] = "Details";
    Layout = null;
}


<dl class="row">
    <dt class = "col-sm-2">
        @Html.DisplayNameFor(model => model.Message)
    </dt>
    <dd class = "col-sm-10">
        @Html.DisplayFor(model => model.Message)
    </dd>
    <dt class = "col-sm-2">
        @Html.DisplayNameFor(model => model.MessageTemplate)
    </dt>
    <dd class = "col-sm-10">
        @Html.DisplayFor(model => model.MessageTemplate)
    </dd>
    <dt class = "col-sm-2">
        @Html.DisplayNameFor(model => model.Level)
    </dt>
    <dd class = "col-sm-10">
        @Html.DisplayFor(model => model.Level)
    </dd>
    <dt class = "col-sm-2">
        @Html.DisplayNameFor(model => model.TimeStamp)
    </dt>
    <dd class = "col-sm-10">
        @Html.DisplayFor(model => model.TimeStamp)
    </dd>
    <dt class = "col-sm-2">
        @Html.DisplayNameFor(model => model.Exception)
    </dt>
    <dd class = "col-sm-10">
        @Html.DisplayFor(model => model.Exception)
    </dd>
    <dt class = "col-sm-2">
        @Html.DisplayNameFor(model => model.Properties)
    </dt>
    <dd class="col-sm-10">
        <pre><code class="language-html">@Model.Properties</code></pre>
    </dd>
</dl>

